  "dependencies": {
    "@aserto/aserto-node": "0.32.0",
    "@aserto/authzen-interop-react": "^1.0.3",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.18.3",
    "express-jwt": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^5.4.2"
  },
  "prettier": "@aserto/ts-linting-configs/prettier.config",
